class Solution
{
    public:
    //Function to return the level order traversal of a tree.
    #include<queue>
    vector<int> levelOrder(Node* node)
    {
      //Your code here
      vector<int> v;
      queue<Node*> q;
      if(node==NULL){
          return v;
      }
      q.push(node);
      while(!q.empty()){
          Node* front=q.front();
          v.push_back(front->data);
            q.pop();
            if(front->left){
               q.push(front->left); 
            }
            if(front->right){
                q.push(front->right);
            }
      }
      return v;
    }
